{
  "name": "component-craft",
  "version": "1.0.1",
  "private": false,
  "main": "dist/mainExport.js",
  "types": "dist/mainExport.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "web": "react-scripts start",
    "test": "jest",
    "test-update-snapshots": "jest -u",
    "lint": "eslint '**/*.{ts,tsx}'",
    "start": "react-native start",
    "prepare": "husky install",
    "build-ts": "tsc --declaration",
    "copy-assets": "node copy-assets.ts",
    "prepare-to-publish": "tsc --declaration && echo ***generated definition files successfully!*** && node copy-assets.ts"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "yarn lint",
      "yarn format"
    ]
  },
  "dependencies": {
    "@react-navigation/native": "^6.1.6",
    "@react-navigation/native-stack": "^6.9.12",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-native": "^4.0.0",
    "react": "18.2.0",
    "react-dom": "^18.2.0",
    "react-native": "0.71.8",
    "react-native-safe-area-context": "^4.5.3",
    "react-native-screens": "^3.20.0",
    "react-native-web": "^0.19.4",
    "react-scripts": "^5.0.1",
    "undefined": "^0.1.0",
    "webpack": "^5.83.1",
    "webpack-cli": "^5.1.1",
    "webpack-dev-server": "^4.15.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native-community/eslint-config": "^3.2.0",
    "@testing-library/react-native": "^12.1.2",
    "@tsconfig/react-native": "^2.0.2",
    "@types/jest": "^29.5.2",
    "@types/react": "^18.2.7",
    "@types/react-native": "^0.72.2",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.59.7",
    "@typescript-eslint/parser": "^5.59.7",
    "babel-jest": "^29.2.1",
    "eslint": "^8.41.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "lint-staged": "^13.2.2",
    "metro-react-native-babel-preset": "0.73.9",
    "prettier": "^2.8.8",
    "react-test-renderer": "18.2.0",
    "typescript": "^4.8.4"
  },
  "jest": {
    "preset": "react-native",
    "testMatch": [
      "<rootDir>/src/components/**/*.test.tsx"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "description": "A small Component library for react-native applications.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/amritha-dinesh/ComponentCraft.git"
  },
  "keywords": [
    "react-native",
    "design",
    "system",
    "component",
    "library"
  ],
  "author": "Amritha Dinesh, Durga Prasad, Jenish Mariyajerald, Shashikant Patil, Manoj Sai Bellamkonda",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/amritha-dinesh/ComponentCraft/issues"
  }
}
